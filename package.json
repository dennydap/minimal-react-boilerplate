{
  "name": "minimal-react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal React Boilerplate is a Boilerplate for developing React without using Create React App.",
  "main": "index.js",
  "scripts": {
    "start": "sass --watch ./src/styles/_style.scss:./src/styles/_style.css --style compressed --scss | webpack-dev-server --config ./webpack.config.js --mode development --open --hot",
    "build": "sass ./src/styles/_style.scss:./src/styles/_style.css --style compressed --scss | webpack --config ./webpack.config.js --mode production --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "google",
    "mws",
    "studio",
    "ghibli",
    "openapi"
  ],
  "author": "Denny Pradipta",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "react-hot-loader": "^4.6.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.8.2",
    "react-bulma-components": "^2.3.0",
    "react-content-loader": "^4.2.1",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.2.1"
  }
}
